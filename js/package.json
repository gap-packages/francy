{
  "private": true,
  "version": "0.9.0",
  "name": "francy-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcmartins/francy.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "test": "lerna run test",
    "prepare": "lerna run init && lerna run link && lerna run prepare",
    "docs": "esdoc",
    "lint": "eslint ./packages/** ",
    "lint:fix": "eslint --fix ./packages/**",
    "clean:jupyter": "cd packages/francy-extension-jupyter && rm -rf ./lib ./dist ./build ./jupyter_francy/jupyter_francy ./jupyter_francy/nbextension ./jupyter_francy/labextension ./jupyter_francy.egg-info",
    "build:jupyter:lib": "cd packages/francy-extension-jupyter && babel src -d lib",
    "build:jupyter:labextension": "cd packages/francy-extension-jupyter/jupyter_francy && rimraf labextension && mkdirp labextension && cd labextension && npm pack ../..",
    "build:jupyter:nbextension": "cd packages/francy-extension-jupyter && webpack --progress",
    "build:jupyter": "npm run clean:jupyter && npm run build:jupyter:lib && npm run build:jupyter:nbextension && npm run build:jupyter:labextension",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm run test"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.1",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@babel/plugin-transform-classes": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.2",
    "chai": "4.1.2",
    "css-loader": "1.0.0",
    "d3": "5.7.0",
    "del": "3.0.0",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "5.5.0",
    "isparta": "4.1.0",
    "karma": "3.0.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-webpack": "3.0.4",
    "lerna": "^3.3.2",
    "mocha": "5.2.0",
    "nyc": "13.0.1",
    "save-svg-as-png": "1.4.6",
    "seedrandom": "2.4.4",
    "style-loader": "0.23.0",
    "webpack": "4.18.1",
    "webpack-cli": "3.1.0"
  }
}
