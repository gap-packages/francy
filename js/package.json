{
  "name": "francy-js",
  "version": "0.6.0",
  "description": "Francy - An Interactive Discrete Mathematics Framework for GAP",
  "author": "Manuel Martins <manuelmachadomartins@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcmartins/francy.git"
  },
  "main": "src/francy.js",
  "keywords": [
    "gap",
    "francy",
    "graphics",
    "interactivity"
  ],
  "license": "MIT",
  "scripts": {
    "docs": "./node_modules/.bin/esdoc",
    "lint": "./node_modules/.bin/eslint ./src/**",
    "lint:fix": "./node_modules/.bin/eslint --fix ./src/**",
    "test": "./node_modules/.bin/karma start karma.conf.js",
    "coverage": "./node_modules/.bin/nyc --reporter=lcov --reporter=text-lcov npm run test",
    "dependencies:style": "echo ./extensions/browser ./extensions/jupyter_francy/style | xargs -n 1 cp ./style/index.css",
    "dependencies:js": "echo ./extensions/jupyter_francy/src | xargs -n 1 cp ./node_modules/d3/dist/d3.min.js",
    "dependencies:all": "npm run dependencies:js && npm run dependencies:style",
    "webpack": "./node_modules/.bin/webpack --env.clean --progress",
    "webpack:production": "./node_modules/.bin/webpack --env.clean --env.production --progress",
    "webpack:all": "npm run webpack && ./node_modules/.bin/webpack --env.production --progress",
    "build:all": "npm run webpack:all && npm run dependencies:all"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "chai": "4.1.2",
    "d3": "5.3.0",
    "del": "3.0.0",
    "esdoc": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "4.19.1",
    "isparta": "4.0.0",
    "karma": "2.0.2",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "3.0.0",
    "mocha": "5.1.1",
    "nyc": "11.7.2",
    "phantomjs": "2.1.7",
    "save-svg-as-png": "1.3.2",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3"
  }
}
