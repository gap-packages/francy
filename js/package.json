{
  "private": true,
  "name": "francy-js",
  "description": "Francy - An Interactive Discrete Mathematics Framework for GAP",
  "repository": {
    "type": "git",
    "url": "https://github.com/gap-packages/francy.git"
  },
  "scripts": {
    "prepare": "lerna link --no-ci && lerna run prepare --no-ci",
    "bootstrap": "lerna bootstrap --no-ci",
    "test": "lerna run test --no-ci",
    "build": "lerna run build --no-ci",
    "build:jupyter": "lerna run build:jupyter --no-ci",
    "build:prod": "lerna run build --no-ci",
    "clean": "lerna run clean --no-ci",
    "version": "lerna version --no-git-tag-version --no-push --no-ci",
    "docs": "esdoc",
    "lint": "eslint --debug --config ./.eslintrc.json ./packages/**",
    "lint:fix": "eslint --debug --fix --config ./.eslintrc.json ./packages/**",
    "coverage": "npm run test",
    "publish": "lerna publish --skip-git --force-publish --no-ci"
  },
